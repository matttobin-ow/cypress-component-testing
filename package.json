{
  "name": "cypress-component-testing",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tw:watch": "tailwindcss -i ./src/index.css -o ./cypress/support/index.css --watch",
    "cy:open": "concurrently \"cypress open\" npm:tw:watch",
    "cy:comp": "cypress run --component",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recoil": "^0.7.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.3",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "cypress": "^10.3.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
